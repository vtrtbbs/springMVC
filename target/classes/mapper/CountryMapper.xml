<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.springstudy.mapper.CountryMapper">

	<select id="select" resultType="com.springstudy.domain.Country">
		select * from country
	</select>

	<insert id="insert" parameterType="com.springstudy.domain.Country">
		insert into
		country(name,abbr)
		values (#{name},#{abbr})
	</insert>
</mapper>

